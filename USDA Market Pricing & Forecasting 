      "cell_type": "code",
      "source": [
        "# Week over Week % Change Table\n",
        "df_wow = df_chart.pivot_table(\n",
        "    index=\"report_date\",\n",
        "    columns=\"item_desc\",\n",
        "    values=\"wow_change\"\n",
        ").round(2)\n",
        "\n",
        "# Format date nicely\n",
        "df_wow.index = df_wow.index.strftime(\"%Y-%m-%d\")\n",
        "\n",
        "# Add + sign for positive values\n",
        "def format_pct(val):\n",
        "    if pd.isna(val):\n",
        "        return \"-\"\n",
        "    elif val > 0:\n",
        "        return f\"+{val}%\"\n",
        "    else:\n",
        "        return f\"{val}%\"\n",
        "\n",
        "df_wow_styled = df_wow.applymap(format_pct)\n",
        "\n",
        "print(\"Week over Week % Price Change by Product:\")\n",
        "df_wow_styled"
