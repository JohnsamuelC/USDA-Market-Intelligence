              "                            \n",
              "var gd = document.getElementById('817b8dcf-65b6-494e-aab6-a1021eea85cd');\n",
              "var x = new MutationObserver(function (mutations, observer) {{\n",
              "        var display = window.getComputedStyle(gd).display;\n",
              "        if (!display || display === 'none') {{\n",
              "            console.log([gd, 'removed!']);\n",
              "            Plotly.purge(gd);\n",
              "            observer.disconnect();\n",
              "        }}\n",
              "}});\n",
              "\n",
              "// Listen for the removal of the full notebook cells\n",
              "var notebookContainer = gd.closest('#notebook-container');\n",
              "if (notebookContainer) {{\n",
              "    x.observe(notebookContainer, {childList: true});\n",
              "}}\n",
              "\n",
              "// Listen for the clearing of the current output cell\n",
              "var outputEl = gd.closest('.output');\n",
              "if (outputEl) {{\n",
              "    x.observe(outputEl, {childList: true});\n",
              "}}\n",
              "\n",
              "                        })                };                            </script>        </div>\n",
              "</body>\n",
              "</html>"
            ]
          },
          "metadata": {}
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import plotly.express as px\n",
        "from plotly.subplots import make_subplots\n",
        "import plotly.graph_objects as go\n",
        "\n",
        "# Clean up the data for charting\n",
        "df_chart = df_180[[\"report_date\", \"item_desc\", \"price_range_avg\"]].dropna()\n",
        "\n",
        "# Convert columns to correct types\n",
        "df_chart[\"report_date\"] = pd.to_datetime(df_chart[\"report_date\"])\n",
        "df_chart[\"price_range_avg\"] = pd.to_numeric(df_chart[\"price_range_avg\"], errors=\"coerce\") / 100\n",
        "\n",
        "# Filter out zero prices\n",
        "df_chart = df_chart[df_chart[\"price_range_avg\"] > 0]\n",
        "\n",
        "# Sort by date\n",
        "df_chart = df_chart.sort_values([\"item_desc\", \"report_date\"])\n",
        "\n",
        "# --- Calculate Week over Week % Change ---\n",
        "df_chart[\"wow_change\"] = df_chart.groupby(\"item_desc\")[\"price_range_avg\"].pct_change() * 100\n",
        "df_chart[\"wow_change\"] = df_chart[\"wow_change\"].round(2)\n",
        "\n",
        "# --- Create Two Charts Side by Side ---\n",
        "fig = make_subplots(\n",
        "    rows=2, cols=1,\n",
        "    subplot_titles=(\n",
        "        \"National Beef Average Prices by Product (Last 180 Days)\",\n",
        "        \"Week over Week Price Change (%)\"\n",
        "    ),\n",
        "    vertical_spacing=0.15\n",
        ")\n",
        "\n",
        "# Get unique products\n",
        "products = df_chart[\"item_desc\"].unique()\n",
        "colors = px.colors.qualitative.Plotly\n",
        "\n",
        "for i, product in enumerate(products):\n",
        "    df_prod = df_chart[df_chart[\"item_desc\"] == product]\n",
        "    color = colors[i % len(colors)]\n",
        "\n",
        "    # Price chart\n",
        "    fig.add_trace(\n",
        "        go.Scatter(\n",
        "            x=df_prod[\"report_date\"],\n",
        "            y=df_prod[\"price_range_avg\"],\n",
        "            name=product,\n",
        "            mode=\"lines+markers\",\n",
        "            line=dict(color=color),\n",
        "            legendgroup=product,\n",
        "            showlegend=True\n",
        "        ),\n",
        "        row=1, col=1\n",
        "    )\n",
        "\n",
        "    # WoW % Change chart\n",
        "    fig.add_trace(\n",
        "        go.Bar(\n",
        "            x=df_prod[\"report_date\"],\n",
        "            y=df_prod[\"wow_change\"],\n",
        "            name=product,\n",
        "            marker_color=color,\n",
        "            legendgroup=product,\n",
        "            showlegend=False\n",
        "        ),\n",
        "        row=2, col=1\n",
        "    )\n",
        "\n",
        "# Layout\n",
        "fig.update_layout(\n",
        "    height=800,\n",
        "    plot_bgcolor=\"white\",\n",
        "    hovermode=\"x unified\",\n",
        "    legend_title=\"Product Type\"\n",
        ")\n",
        "\n",
        "fig.update_yaxes(title_text=\"Avg Price ($ per lb)\", row=1, col=1)\n",
        "fig.update_yaxes(title_text=\"WoW Change (%)\", row=2, col=1)\n",
        "fig.update_xaxes(title_text=\"Date\", row=2, col=1)\n",
        "\n",
        "# Add a zero line on WoW chart\n",
        "fig.add_hline(y=0, line_dash=\"dash\", line_color=\"black\", row=2, col=1)\n",
        "\n",
        "fig.show()"
      ],
