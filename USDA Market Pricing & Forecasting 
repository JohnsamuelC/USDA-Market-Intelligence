    {
      "cell_type": "code",
      "source": [
        "import requests\n",
        "import pandas as pd\n",
        "\n",
        "# List of dates we want to pull\n",
        "dates = [\n",
        "    \"9/26/2025\",\n",
        "    \"9/19/2025\",\n",
        "    \"9/12/2025\",\n",
        "    \"9/05/2025\",\n",
        "    \"8/29/2025\",\n",
        "    \"8/22/2025\",\n",
        "    \"8/15/2025\",\n",
        "    \"8/08/2025\",\n",
        "    \"8/01/2025\",\n",
        "    \"7/25/2025\"\n",
        "]\n",
        "\n",
        "all_data = []\n",
        "\n",
        "for date in dates:\n",
        "    url = \"https://mpr.datamart.ams.usda.gov/services/v1.1/reports/2462/National\"\n",
        "    params = {\n",
        "        \"q\": f\"report_date={date}\",\n",
        "        \"allSections\": \"true\"\n",
        "    }\n",
        "\n",
        "    response = requests.get(url, params=params)\n",
        "\n",
        "    if response.status_code == 200:\n",
        "        data = response.json()\n",
        "        for section in data:\n",
        "            if section[\"reportSection\"] == \"National\":\n",
        "                for row in section[\"results\"]:\n",
        "                    row[\"report_date\"] = date  # tag the date\n",
        "                    all_data.append(row)\n",
        "        print(f\"✅ Pulled data for {date}\")\n",
        "    else:\n",
        "        print(f\"❌ Failed for {date}\")\n",
        "\n",
        "# Combine into one big DataFrame\n",
        "df_historical = pd.DataFrame(all_data)\n",
        "print(f\"\\nTotal records pulled: {len(df_historical)}\")\n",
        "df_historical.head()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 628
        },
        "id": "gSdp0zFpWpts",
        "outputId": "7d5a75ea-42b9-4df9-efeb-d78eced16fa2"
      },
      "execution_count": 8,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
