      "cell_type": "code",
      "source": [
        "import plotly.express as px\n",
        "\n",
        "# Clean up the data for charting\n",
        "df_chart = df_180[[\"report_date\", \"item_desc\", \"price_range_avg\"]].dropna()\n",
        "\n",
        "# Convert columns to correct types\n",
        "df_chart[\"report_date\"] = pd.to_datetime(df_chart[\"report_date\"])\n",
        "df_chart[\"price_range_avg\"] = pd.to_numeric(df_chart[\"price_range_avg\"], errors=\"coerce\")\n",
        "\n",
        "# Filter out zero prices\n",
        "df_chart = df_chart[df_chart[\"price_range_avg\"] > 0]\n",
        "\n",
        "# Sort by date\n",
        "df_chart = df_chart.sort_values(\"report_date\")\n",
        "\n",
        "# Plot - one line per product type\n",
        "fig = px.line(\n",
        "    df_chart,\n",
        "    x=\"report_date\",\n",
        "    y=\"price_range_avg\",\n",
        "    color=\"item_desc\",\n",
        "    title=\"National Beef Average Prices by Product (Last 180 Days)\",\n",
        "    labels={\n",
        "        \"report_date\": \"Date\",\n",
        "        \"price_range_avg\": \"Avg Price ($ per 100 lbs)\",\n",
        "        \"item_desc\": \"Product Type\"\n",
        "    },\n",
        "    markers=True\n",
        ")\n",
        "\n",
        "fig.update_layout(\n",
        "    plot_bgcolor=\"white\",\n",
        "    hovermode=\"x unified\",\n",
        "    legend_title=\"Product Type\",\n",
        "    height=600\n",
        ")\n",
        "\n",
        "fig.show()"
