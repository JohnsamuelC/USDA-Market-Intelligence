    {
      "cell_type": "code",
      "source": [
        "import requests\n",
        "import pandas as pd\n",
        "\n",
        "# Pull last 180 days of National beef data\n",
        "url = \"https://mpr.datamart.ams.usda.gov/services/v1.1/reports/2462/National\"\n",
        "\n",
        "params = {\n",
        "    \"q\": \"report_date=4/1/2025:9/26/2025\",\n",
        "    \"sort\": \"report_date\"\n",
        "}\n",
        "\n",
        "response = requests.get(url, params=params)\n",
        "data = response.json()\n",
        "\n",
        "# Handle different response formats\n",
        "if isinstance(data, list):\n",
        "    # Multiple sections returned\n",
        "    all_rows = []\n",
        "    for section in data:\n",
        "        if isinstance(section, dict) and \"results\" in section:\n",
        "            all_rows.extend(section[\"results\"])\n",
        "    df_180 = pd.DataFrame(all_rows)\n",
        "\n",
        "elif isinstance(data, dict):\n",
        "    # Single section returned\n",
        "    if \"results\" in data:\n",
        "        df_180 = pd.DataFrame(data[\"results\"])\n",
        "    else:\n",
        "        df_180 = pd.DataFrame([data])\n",
