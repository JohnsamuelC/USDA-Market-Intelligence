      "cell_type": "code",
      "source": [
        "import requests\n",
        "import pandas as pd\n",
        "import json\n",
        "\n",
        "# --- API CALL ---\n",
        "url = \"https://mpr.datamart.ams.usda.gov/services/v1.1/reports/2462/Summary\"\n",
        "params = {\n",
        "    \"q\": \"report_date=9/26/2025\",\n",
        "    \"allSections\": \"true\"\n",
        "}\n",
        "\n",
        "response = requests.get(url, params=params)\n",
        "data = response.json()\n",
        "\n",
        "# --- PARSE EACH SECTION ---\n",
        "for section in data:\n",
        "    section_name = section[\"reportSection\"]\n",
        "    results = section[\"results\"]\n",
        "    df_section = pd.DataFrame(results)\n",
        "\n",
        "    print(f\"\\n========== {section_name} ==========\")\n",
        "    print(df_section.to_string(index=False))\n",
        "    print()"
      ],
