    {
      "cell_type": "code",
      "source": [
        "import pmdarima as pm\n",
        "import plotly.graph_objects as go\n",
        "from plotly.subplots import make_subplots\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "# Get all unique products\n",
        "products = df_chart[\"item_desc\"].unique()\n",
        "\n",
        "# Store results\n",
        "all_forecasts = {}\n",
        "all_accuracies = {}\n",
        "\n",
        "print(\"Training ARIMA models for all products...\\n\")\n",
        "\n",
        "for product in products:\n",
        "    try:\n",
        "        # Prepare data for each product\n",
        "        df_prod = df_chart[df_chart[\"item_desc\"] == product][[\"report_date\", \"price_range_avg\"]].dropna()\n",
        "        df_prod = df_prod.sort_values(\"report_date\").set_index(\"report_date\")\n",
        "        df_prod.index = pd.DatetimeIndex(df_prod.index.values)\n",
        "        df_prod = df_prod.asfreq(\"W-FRI\", method=\"ffill\")\n",
        "\n",
        "        if len(df_prod) < 10:\n",
        "            print(f\"⚠️ Skipping {product} — not enough data\")\n",
        "            continue\n",
        "\n",
        "        # Train/test split\n",
        "        split = int(len(df_prod) * 0.8)\n",
        "        train = df_prod.iloc[:split]\n",
        "        test = df_prod.iloc[split:]\n",
        "\n",
        "        # Auto ARIMA\n",
        "        model = pm.auto_arima(\n",
        "            train[\"price_range_avg\"],\n",
        "            seasonal=False,\n",
        "            stepwise=True,\n",
        "            suppress_warnings=True,\n",
        "            error_action=\"ignore\"\n",
        "        )\n",
        "\n",
        "        # Backtest accuracy\n",
        "        predictions = model.predict(n_periods=len(test))\n",
        "        mape = np.mean(np.abs((test[\"price_range_avg\"].values - predictions) / test[\"price_range_avg\"].values)) * 100\n",
        "        accuracy = round(100 - mape, 2)\n",
        "        all_accuracies[product] = accuracy\n",
        "\n",
        "        # Forecast next 26 weeks with confidence intervals\n",
        "        forecast, conf_int = model.predict(n_periods=26, return_conf_int=True, alpha=0.05)\n",
        "        forecast_index = pd.date_range(\n",
        "            start=df_prod.index[-1] + pd.Timedelta(weeks=1),\n",
        "            periods=26,\n",
        "            freq=\"W-FRI\"\n",
        "        )\n",
        "\n",
        "        all_forecasts[product] = {\n",
        "            \"historical\": df_prod,\n",
        "            \"forecast\": forecast,\n",
        "            \"lower\": conf_int[:, 0],\n",
        "            \"upper\": conf_int[:, 1],\n",
        "            \"forecast_index\": forecast_index,\n",
        "            \"test\": test,\n",
        "            \"predictions\": predictions\n",
        "        }\n",
        "\n",
        "        print(f\"✅ {product} — Accuracy: {accuracy}%\")\n",
        "\n",
        "    except Exception as e:\n",
        "        print(f\"❌ {product} failed: {e}\")\n",
        "\n",
        "print(\"\\n🎉 All models trained!\")"
      ],
      "metadata": {
