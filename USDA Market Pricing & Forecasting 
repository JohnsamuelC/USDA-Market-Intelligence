        "    showlegend=True), row=2, col=1)\n",
        "\n",
        "# Forecast line on top\n",
        "fig.add_trace(go.Scatter(\n",
        "    x=df_forecast.index, y=df_forecast[\"forecast\"],\n",
        "    name=\"180 Day Forecast\",\n",
        "    line=dict(color=\"orange\", dash=\"dash\", width=2)), row=2, col=1)\n",
        "\n",
        "# --- LAYOUT ---\n",
        "fig.update_layout(\n",
        "    height=900,\n",
        "    plot_bgcolor=\"white\",\n",
