    {
      "cell_type": "code",
      "source": [
        "# Only plot products with accuracy above 80%\n",
        "good_products = {k: v for k, v in all_forecasts.items() if all_accuracies[k] >= 80}\n",
        "\n",
        "# Create subplots - one per product\n",
        "fig = make_subplots(\n",
        "    rows=len(good_products), cols=1,\n",
        "    subplot_titles=[f\"{p} — Accuracy: {all_accuracies[p]}%\" for p in good_products.keys()],\n",
        "    vertical_spacing=0.08\n",
        ")\n",
        "\n",
        "colors = [\"blue\", \"green\", \"red\", \"purple\", \"brown\"]\n",
        "\n",
        "for i, (product, data) in enumerate(good_products.items(), start=1):\n",
        "    color = colors[(i-1) % len(colors)]\n",
        "\n",
        "    # Historical price\n",
        "    fig.add_trace(go.Scatter(\n",
        "        x=data[\"historical\"].index,\n",
        "        y=data[\"historical\"][\"price_range_avg\"],\n",
        "        name=f\"{product} Historical\",\n",
        "        line=dict(color=color),\n",
        "        legendgroup=product,\n",
        "        showlegend=True\n",
        "    ), row=i, col=1)\n",
        "\n",
        "    # Upper confidence band\n",
        "    fig.add_trace(go.Scatter(\n",
        "        x=data[\"forecast_index\"],\n",
        "        y=data[\"upper\"],\n",
        "        line=dict(color=\"rgba(255,165,0,0)\"),\n",
        "        showlegend=False,\n",
        "        legendgroup=product\n",
        "    ), row=i, col=1)\n",
        "\n",
        "    # Lower confidence band with fill\n",
        "    fig.add_trace(go.Scatter(\n",
        "        x=data[\"forecast_index\"],\n",
        "        y=data[\"lower\"],\n",
        "        fill=\"tonexty\",\n",
        "        fillcolor=\"rgba(255,165,0,0.2)\",\n",
        "        line=dict(color=\"rgba(255,165,0,0)\"),\n",
        "        showlegend=False,\n",
        "        legendgroup=product\n",
        "    ), row=i, col=1)\n",
        "\n",
        "    # Forecast line\n",
        "    fig.add_trace(go.Scatter(\n",
        "        x=data[\"forecast_index\"],\n",
        "        y=data[\"forecast\"],\n",
        "        name=f\"{product} Forecast\",\n",
        "        line=dict(color=\"orange\", dash=\"dash\", width=2),\n",
        "        legendgroup=product,\n",
        "        showlegend=True\n",
        "    ), row=i, col=1)\n",
        "\n",
        "fig.update_layout(\n",
        "    height=400 * len(good_products),\n",
        "    plot_bgcolor=\"white\",\n",
        "    hovermode=\"x unified\",\n",
        "    title_text=\"ARIMA Beef Price Forecast — All Products (180 Days)\"\n",
        ")\n",
        "\n",
        "for i in range(1, len(good_products) + 1):\n",
        "    fig.update_yaxes(title_text=\"Price ($ per lb)\", row=i, col=1)\n",
        "\n",
        "fig.show()\n",
        "\n",
        "# --- SUMMARY ACCURACY TABLE ---\n",
        "print(\"\\n📋 Model Accuracy Summary:\")\n",
        "df_accuracy = pd.DataFrame({\n",
        "    \"Product\": list(all_accuracies.keys()),\n",
        "    \"Accuracy (%)\": list(all_accuracies.values()),\n",
        "    \"Status\": [\"✅ Good\" if v >= 80 else \"⚠️ Low Data\" for v in all_accuracies.values()]\n",
        "}).sort_values(\"Accuracy (%)\", ascending=False)\n",
        "\n",
        "df_accuracy"
