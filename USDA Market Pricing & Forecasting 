      "source": [
        "import pandas as pd\n",
        "import numpy as np\n",
        "\n",
        "# Pick Fresh 90% as our starting product\n",
        "product = \"Fresh 90%\"\n",
        "\n",
        "# Filter and prepare data\n",
        "df_arima = df_chart[df_chart[\"item_desc\"] == product][[\"report_date\", \"price_range_avg\"]].dropna()\n",
        "df_arima = df_arima.sort_values(\"report_date\")\n",
        "df_arima = df_arima.set_index(\"report_date\")\n",
        "df_arima.index = pd.DatetimeIndex(df_arima.index.values, freq=df_arima.index.inferred_freq)\n",
        "\n",
        "print(f\"✅ Data prepared for: {product}\")\n",
        "print(f\"Date range: {df_arima.index.min()} to {df_arima.index.max()}\")\n",
        "print(f\"Total weekly data points: {len(df_arima)}\")\n",
