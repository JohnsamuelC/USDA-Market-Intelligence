    {
      "cell_type": "code",
      "source": [
        "import pmdarima as pm\n",
        "from statsmodels.tsa.arima.model import ARIMA\n",
        "import plotly.graph_objects as go\n",
        "from plotly.subplots import make_subplots\n",
        "import numpy as np\n",
        "\n",
        "# --- SPLIT DATA: 80% train, 20% test (backtest) ---\n",
        "split = int(len(df_arima) * 0.8)\n",
        "train = df_arima.iloc[:split]\n",
        "test = df_arima.iloc[split:]\n",
        "\n",
        "print(f\"Training data: {train.index.min().date()} to {train.index.max().date()} ({len(train)} weeks)\")\n",
        "print(f\"Testing data:  {test.index.min().date()} to {test.index.max().date()} ({len(test)} weeks)\")\n",
        "\n",
        "# --- AUTO FIND BEST ARIMA SETTINGS ---\n",
        "print(\"\\nFinding best ARIMA model... please wait ⏳\")\n",
        "auto_model = pm.auto_arima(\n",
        "    train[\"price_range_avg\"],\n",
        "    seasonal=False,\n",
        "    stepwise=True,\n",
        "    suppress_warnings=True,\n",
        "    error_action=\"ignore\"\n",
        ")\n",
        "print(f\"✅ Best ARIMA order found: {auto_model.order}\")\n",
        "\n",
        "# --- BACKTEST: Predict on test data ---\n",
        "predictions = auto_model.predict(n_periods=len(test))\n",
        "df_test_pred = pd.DataFrame({\n",
        "    \"actual\": test[\"price_range_avg\"].values,\n",
        "    \"predicted\": predictions\n",
        "}, index=test.index)\n",
        "\n",
        "# --- ACCURACY METRICS ---\n",
        "mae = np.mean(np.abs(df_test_pred[\"actual\"] - df_test_pred[\"predicted\"]))\n",
        "mape = np.mean(np.abs((df_test_pred[\"actual\"] - df_test_pred[\"predicted\"]) / df_test_pred[\"actual\"])) * 100\n",
        "accuracy = 100 - mape\n",
        "\n",
        "print(f\"\\n📊 Model Accuracy Results:\")\n",
        "print(f\"  Mean Absolute Error (MAE): ${mae:.4f} per lb\")\n",
        "print(f\"  Mean Absolute % Error:     {mape:.2f}%\")\n",
        "print(f\"  ✅ Model Accuracy:          {accuracy:.2f}%\")\n",
        "\n",
        "# --- FORECAST: Next 180 days (26 weeks) ---\n",
        "forecast = auto_model.predict(n_periods=26)\n",
        "forecast_index = pd.date_range(\n",
        "    start=df_arima.index[-1] + pd.Timedelta(weeks=1),\n",
        "    periods=26,\n",
        "    freq=\"W-FRI\"\n",
        ")\n",
        "df_forecast = pd.DataFrame({\n",
        "    \"forecast\": forecast\n",
        "}, index=forecast_index)\n",
        "\n",
        "# --- PLOT EVERYTHING ---\n",
        "fig = make_subplots(\n",
        "    rows=2, cols=1,\n",
        "    subplot_titles=(\n",
        "        \"ARIMA Backtest: Actual vs Predicted Prices\",\n",
        "        \"ARIMA Forecast: Next 180 Days\"\n",
        "    ),\n",
        "    vertical_spacing=0.15\n",
        ")\n",
        "\n",
        "# Backtest chart\n",
        "fig.add_trace(go.Scatter(x=train.index, y=train[\"price_range_avg\"],\n",
        "    name=\"Training Data\", line=dict(color=\"blue\")), row=1, col=1)\n",
        "fig.add_trace(go.Scatter(x=test.index, y=test[\"price_range_avg\"],\n",
        "    name=\"Actual Price\", line=dict(color=\"green\")), row=1, col=1)\n",
        "fig.add_trace(go.Scatter(x=df_test_pred.index, y=df_test_pred[\"predicted\"],\n",
        "    name=\"Predicted Price\", line=dict(color=\"red\", dash=\"dash\")), row=1, col=1)\n",
        "\n",
        "# Forecast chart\n",
        "fig.add_trace(go.Scatter(x=df_arima.index, y=df_arima[\"price_range_avg\"],\n",
        "    name=\"Historical Price\", line=dict(color=\"blue\")), row=2, col=1)\n",
        "fig.add_trace(go.Scatter(x=df_forecast.index, y=df_forecast[\"forecast\"],\n",
        "    name=\"180 Day Forecast\", line=dict(color=\"orange\", dash=\"dash\"),\n",
        "    fill=\"tonexty\", fillcolor=\"rgba(255,165,0,0.1)\"), row=2, col=1)\n",
        "\n",
        "fig.update_layout(\n",
        "    height=900,\n",
        "    plot_bgcolor=\"white\",\n",
        "    hovermode=\"x unified\",\n",
        "    title_text=f\"ARIMA Beef Price Forecast — Accuracy: {accuracy:.2f}%\"\n",
        ")\n",
        "\n",
        "fig.update_yaxes(title_text=\"Price ($ per lb)\", row=1, col=1)\n",
        "fig.update_yaxes(title_text=\"Price ($ per lb)\", row=2, col=1)\n",
        "\n",
        "fig.show()\n",
        "\n",
        "# Show backtest table\n",
        "print(\"\\n📋 Backtest Results Table:\")\n",
        "df_test_pred[\"difference\"] = (df_test_pred[\"actual\"] - df_test_pred[\"predicted\"]).round(4)\n",
        "df_test_pred[\"error_%\"] = ((df_test_pred[\"difference\"] / df_test_pred[\"actual\"]) * 100).round(2)\n",
        "df_test_pred"
      ],
