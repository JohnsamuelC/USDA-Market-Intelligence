        "import plotly.graph_objects as go\n",
        "from plotly.subplots import make_subplots\n",
        "import numpy as np\n",
        "\n",
        "# --- GET FORECAST WITH CONFIDENCE INTERVALS ---\n",
        "forecast, conf_int = auto_model.predict(\n",
        "    n_periods=26,\n",
        "    return_conf_int=True,\n",
        "    alpha=0.05  # 95% confidence interval\n",
        ")\n",
        "\n",
        "forecast_index = pd.date_range(\n",
        "    start=df_arima.index[-1] + pd.Timedelta(weeks=1),\n",
        "    periods=26,\n",
        "    freq=\"W-FRI\"\n",
        ")\n",
        "\n",
        "df_forecast = pd.DataFrame({\n",
        "    \"forecast\": forecast,\n",
        "    \"lower_95\": conf_int[:, 0],\n",
        "    \"upper_95\": conf_int[:, 1]\n",
        "}, index=forecast_index)\n",
        "\n",
        "# --- PLOT ---\n",
        "fig = make_subplots(\n",
        "    rows=2, cols=1,\n",
        "    subplot_titles=(\n",
        "        \"ARIMA Backtest: Actual vs Predicted Prices\",\n",
        "        f\"ARIMA Forecast: Next 180 Days (95% Confidence Interval) — Accuracy: {accuracy:.2f}%\"\n",
        "    ),\n",
        "    vertical_spacing=0.15\n",
        ")\n",
        "\n",
        "# --- BACKTEST CHART ---\n",
        "fig.add_trace(go.Scatter(\n",
        "    x=train.index, y=train[\"price_range_avg\"],\n",
        "    name=\"Training Data\", line=dict(color=\"blue\")), row=1, col=1)\n",
        "\n",
        "fig.add_trace(go.Scatter(\n",
        "    x=test.index, y=test[\"price_range_avg\"],\n",
        "    name=\"Actual Price\", line=dict(color=\"green\")), row=1, col=1)\n",
        "\n",
        "fig.add_trace(go.Scatter(\n",
        "    x=df_test_pred.index, y=df_test_pred[\"predicted\"],\n",
        "    name=\"Predicted Price\", line=dict(color=\"red\", dash=\"dash\")), row=1, col=1)\n",
        "\n",
        "# --- FORECAST CHART ---\n",
        "# Historical price\n",
        "fig.add_trace(go.Scatter(\n",
        "    x=df_arima.index, y=df_arima[\"price_range_avg\"],\n",
        "    name=\"Historical Price\", line=dict(color=\"blue\")), row=2, col=1)\n",
        "\n",
        "# Upper confidence band\n",
        "fig.add_trace(go.Scatter(\n",
        "    x=df_forecast.index, y=df_forecast[\"upper_95\"],\n",
