      "cell_type": "code",
      "source": [
        "import pdfplumber\n",
        "import pandas as pd\n",
        "\n",
        "# Data we'll extract\n",
        "rows = []\n",
        "\n",
        "# Region sections in the PDF\n",
        "current_region = None\n",
        "\n",
        "with pdfplumber.open(\"beef_trimming.pdf\") as pdf:\n",
        "    for page in pdf.pages:\n",
        "        text = page.extract_text()\n",
        "        lines = text.split(\"\\n\")\n",
        "\n",
        "        for line in lines:\n",
        "            # Detect region headers\n",
        "            if \"FOB Plant - National\" in line:\n",
        "                current_region = \"National\"\n",
        "            elif \"FOB Plant - Central\" in line:\n",
        "                current_region = \"Central\"\n",
        "            elif \"FOB Plant - East Coast\" in line:\n",
        "                current_region = \"East Coast\"\n",
        "            elif \"FOB Plant - West Coast\" in line:\n",
        "                current_region = \"West Coast\"\n",
        "\n",
        "            # Extract data rows (Fresh/Frozen lines with numbers)\n",
        "            elif line.startswith(\"Fresh\") or line.startswith(\"Frozen\"):\n",
        "                parts = line.split()\n",
        "                if len(parts) >= 5:\n",
        "                    try:\n",
        "                        product = parts[0] + \" \" + parts[1]  # e.g. \"Fresh 90%\"\n",
        "                        trades = int(parts[2])\n",
        "                        pounds = int(parts[3].replace(\",\", \"\"))\n",
        "                        weighted_avg = float(parts[-1])\n",
        "\n",
        "                        rows.append({\n",
        "                            \"Region\": current_region,\n",
        "                            \"Product\": product,\n",
        "                            \"Trades\": trades,\n",
        "                            \"Total Pounds\": pounds,\n",
        "                            \"Weighted Avg Price\": weighted_avg\n",
        "                        })\n",
        "                    except:\n",
        "                        pass\n",
        "\n",
        "# Build DataFrame\n",
        "df_beef = pd.DataFrame(rows)\n",
        "\n",
        "# Filter out zero rows to keep it clean\n",
        "df_beef = df_beef[df_beef[\"Trades\"] > 0]\n",
        "\n",
        "print(\"Success! Here is your clean beef data:\")\n",
        "df_beef"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 400
        },
        "id": "BkwcqEGZVOPC",
        "outputId": "89414b21-9ad8-4cea-a172-68c9cc20a2a0"
      },
