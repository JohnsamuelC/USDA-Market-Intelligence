    {
      "cell_type": "code",
      "source": [
        "import requests\n",
        "import pandas as pd\n",
        "\n",
        "# --- PULL ALL REGIONS ---\n",
        "regions = {\n",
        "    \"National\": \"National\",\n",
        "    \"Central\": \"Central\",\n",
        "    \"East Coast\": \"East Coast\"\n",
        "}\n",
        "\n",
        "all_regional_data = {}\n",
        "\n",
        "for region_name, section_name in regions.items():\n",
        "    url = f\"https://mpr.datamart.ams.usda.gov/services/v1.1/reports/2462/{region_name.replace(' ', '%20')}\"\n",
        "    params = {\n",
        "        \"q\": \"report_date=4/1/2025:9/26/2025\",\n",
        "        \"sort\": \"report_date\"\n",
        "    }\n",
        "\n",
        "    response = requests.get(url, params=params)\n",
        "\n",
        "    if response.status_code == 200:\n",
        "        data = response.json()\n",
        "        rows = []\n",
        "\n",
        "        if isinstance(data, list):\n",
        "            for section in data:\n",
        "                if isinstance(section, dict) and \"results\" in section:\n",
        "                    for row in section[\"results\"]:\n",
        "                        row[\"region\"] = region_name\n",
        "                        rows.append(row)\n",
        "        elif isinstance(data, dict) and \"results\" in data:\n",
        "            for row in data[\"results\"]:\n",
        "                row[\"region\"] = region_name\n",
        "                rows.append(row)\n",
        "\n",
        "        df_region = pd.DataFrame(rows)\n",
        "        all_regional_data[region_name] = df_region\n",
        "        print(f\"✅ {region_name}: {len(df_region)} records pulled\")\n",
        "    else:\n",
        "        print(f\"❌ {region_name} failed: {response.status_code}\")\n",
        "\n",
        "# Combine all regions\n",
        "df_all_regions = pd.concat(all_regional_data.values(), ignore_index=True)\n",
        "print(f\"\\n✅ Total combined records: {len(df_all_regions)}\")\n",
        "print(f\"Regions: {df_all_regions['region'].unique()}\")\n",
        "df_all_regions.head(10)"
      ],
