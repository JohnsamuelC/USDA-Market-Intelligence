        "import pdfplumber\n",
        "\n",
        "# Open the PDF and extract text\n",
        "with pdfplumber.open(\"beef_trimming.pdf\") as pdf:\n",
        "    for i, page in enumerate(pdf.pages):\n",
        "        print(f\"--- Page {i+1} ---\")\n",
        "        print(page.extract_text())"
