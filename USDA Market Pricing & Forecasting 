    {
      "cell_type": "code",
      "source": [
        "import pmdarima as pm\n",
        "import numpy as np\n",
        "\n",
        "# --- FUNCTION TO BUILD COMBINED REGIONAL SERIES ---\n",
        "def build_combined_series(product_name):\n",
        "    frames = []\n",
        "\n",
        "    for region in [\"National\", \"Central\", \"East Coast\"]:\n",
        "        df_r = df_all_regions[\n",
        "            (df_all_regions[\"region\"] == region) &\n",
        "            (df_all_regions[\"item_desc\"] == product_name)\n",
        "        ][[\"report_date\", \"price_range_avg\"]].copy()\n",
        "\n",
        "        df_r[\"report_date\"] = pd.to_datetime(df_r[\"report_date\"])\n",
        "        df_r[\"price_range_avg\"] = pd.to_numeric(df_r[\"price_range_avg\"], errors=\"coerce\") / 100\n",
        "        df_r = df_r[df_r[\"price_range_avg\"] > 0].dropna()\n",
        "        frames.append(df_r)\n",
        "\n",
        "    if not frames:\n",
        "        return pd.DataFrame()\n",
        "\n",
        "    # Combine all regions and average by date\n",
        "    df_combined = pd.concat(frames)\n",
        "    df_combined = df_combined.groupby(\"report_date\")[\"price_range_avg\"].mean().reset_index()\n",
        "    df_combined = df_combined.sort_values(\"report_date\").set_index(\"report_date\")\n",
        "    df_combined.index = pd.DatetimeIndex(df_combined.index)\n",
        "    df_combined = df_combined.asfreq(\"W-FRI\", method=\"ffill\")\n",
        "\n",
        "    return df_combined\n",
        "\n",
        "# --- RETRAIN FOR LOW ACCURACY PRODUCTS ---\n",
        "low_products = [\"Chemical Lean, Fresh  50%\", \"Chemical Lean, Fresh  73%\"]\n",
        "\n",
        "for product in low_products:\n",
        "    print(f\"\\n🔄 Retraining: {product}\")\n",
        "\n",
        "    df_combined = build_combined_series(product)\n",
        "\n",
        "    if len(df_combined) < 10:\n",
        "        print(f\"  ❌ Still not enough data after combining regions\")\n",
        "        continue\n",
        "\n",
        "    print(f\"  ✅ Combined data points: {len(df_combined)}\")\n",
        "\n",
        "    # Train/test split\n",
        "    split = int(len(df_combined) * 0.8)\n",
        "    train = df_combined.iloc[:split]\n",
        "    test = df_combined.iloc[split:]\n",
        "\n",
        "    # Auto ARIMA\n",
        "    model = pm.auto_arima(\n",
        "        train[\"price_range_avg\"],\n",
        "        seasonal=False,\n",
        "        stepwise=True,\n",
        "        suppress_warnings=True,\n",
        "        error_action=\"ignore\"\n",
        "    )\n",
        "\n",
        "    # Accuracy\n",
        "    if len(test) > 0:\n",
        "        predictions = model.predict(n_periods=len(test))\n",
        "        mape = np.mean(np.abs((test[\"price_range_avg\"].values - predictions) /\n",
        "                               np.where(test[\"price_range_avg\"].values == 0, 1,\n",
        "                                        test[\"price_range_avg\"].values))) * 100\n",
        "        accuracy = round(100 - mape, 2)\n",
        "        print(f\"  📊 Improved Accuracy: {accuracy}%\")\n",
        "        all_accuracies[product] = accuracy\n",
        "\n",
        "    # Forecast\n",
        "    forecast, conf_int = model.predict(n_periods=26, return_conf_int=True, alpha=0.05)\n",
        "    forecast_index = pd.date_range(\n",
        "        start=df_combined.index[-1] + pd.Timedelta(weeks=1),\n",
        "        periods=2"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 102
        },
        "id": "qAjoOj9rsYbK",
        "outputId": "8da4d200-cd23-41aa-99e1-bd893cf6ee5b"
      },
      "execution_count": 27,
      "outputs": [
        {
          "output_type": "error",
          "ename": "SyntaxError",
          "evalue": "incomplete input (ipython-input-2721740972.py, line 73)",
          "traceback": [
            "\u001b[0;36m  File \u001b[0;32m\"/tmp/ipython-input-2721740972.py\"\u001b[0;36m, line \u001b[0;32m73\u001b[0m\n\u001b[0;31m    periods=2\u001b[0m\n\u001b[0m             ^\u001b[0m\n\u001b[0;31mSyntaxError\u001b[0m\u001b[0;31m:\u001b[0m incomplete input\n"
          ]
